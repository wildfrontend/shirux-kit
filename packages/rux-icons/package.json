{
  "name": "@shirux/rux-icons",
  "version": "1.0.0",
  "description": "SVG icons as React components for Rux project",
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    "./fill": "./src/fill/index.ts",
    "./fill/*": "./src/fill/*.tsx",
    "./outline": "./src/outline/index.ts",
    "./outline/*": "./src/outline/*.tsx",
    "./two-tone": "./src/two-tone/index.ts",
    "./two-tone/*": "./src/two-tone/*.tsx",
    "./logo": "./src/logo/index.ts",
    "./logo/*": "./src/logo/*.tsx",
    "./lucide": "./src/lucide/index.ts"
  },
  "scripts": {
    "clean": "rm -rf src dist",
    "pregenerate": "rm -rf src",
    "generate": "svgr assets --out-dir src --ext tsx --config-file svgr.config.js --no-index",
    "postgenerate": "node scripts/post-process-dual-color.js",
    "generate-index": "node scripts/generate-index.js",
    "prebuild":"pnpm run generate",
    "build": "pnpm run generate-index",
    "dev": "pnpm run build --watch"
  },
  "peerDependencies": {
    "lucide-react": "^0.553.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.0.1",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "lucide-react": "^0.553.0"
  }
}
